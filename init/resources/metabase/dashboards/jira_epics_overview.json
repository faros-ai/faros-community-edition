    {
      "name": "Issues / Points by Epic by Status",
      "description": null,
      "display": "bar",
      "table_id": {{ table "tms_Task" }},
      "dataset_query": {
        "type": "query",
        "query": {
          "source-table": {{ table "tms_Task" }},
          "joins": [
            {
              "fields": "all",
              "strategy": "right-join",
              "source-table": {{ table "tms_Epic" }},
              "condition": [
                "=",
                [
                  "field",
                  {{ field "tms_Task.epic" }},
                  null
                ],
                [
                  "field",
                  {{ field "tms_Epic.id" }},
                  {
                    "join-alias": "Tms Epic - Epic"
                  }
                ]
              ],
              "alias": "Tms Epic - Epic"
            }
          ],
          "expressions": {
            "PointsImputed": [
              "case",
              [
                [
                  [
                    "is-null",
                    [
                      "field",
                      {{ field "tms_Task.points" }},
                      null
                    ]
                  ],
                  1
                ]
              ],
              {
                "default": [
                  "field",
                  {{ field "tms_Task.points" }},
                  null
                ]
              }
            ]
          },
          "aggregation": [
            [
              "sum",
              [
                "expression",
                "PointsImputed"
              ]
            ]
          ],
          "breakout": [
            [
              "field",
              {{ field "tms_Epic.name" }},
              {
                "join-alias": "Tms Epic - Epic"
              }
            ],
            [
              "field",
              {{ field "tms_Task.statusDetail" }},
              null
            ]
          ],
          "filter": [
            "and",
            [
              "!=",
              [
                "field",
                {{ field "tms_Task.typeDetail" }},
                null
              ],
              "Epic"
            ],
            [
              "=",
              [
                "field",
                {{ field "tms_Task.source" }},
                null
              ],
              "Jira"
            ]
          ]
        }
      },
      "visualization_settings": {
        "stackable.stack_type": "stacked",
        "graph.dimensions": [
          "name",
          "statusCategory"
        ],
        "series_settings": {
          "Done": {
            "color": "#A989C5"
          },
          "InProgress": {
            "color": "#EF8C8C"
          },
          "Todo": {
            "color": "#98D9D9"
          }
        },
        "graph.y_axis.title_text": "Issues / Points",
        "graph.x_axis.title_text": "Epic",
        "graph.metrics": [
          "sum"
        ]
      }
    },
