    {
      "name": "Reviews Distribution",
      "description": null,
      "display": "pie",
      "table_id": {{ table "vcs_PullRequest" }},
      "dataset_query": {
        "type": "query",
        "query": {
          "source-query": {
            "source-table": {{ table "vcs_PullRequest" }},
            "expressions": {
              "review": [
                "case",
