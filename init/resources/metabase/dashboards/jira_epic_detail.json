    {
      "name": "Epic Completion",
      "description": null,
      "display": "progress",
      "table_id": {{ table "tms_Task" }},
      "dataset_query": {
        "type": "query",
        "query": {
          "source-table": {{ table "tms_Task" }},
          "joins": [
            {
              "fields": "all",
              "strategy": "right-join",
              "source-table": {{ table "tms_Epic" }},
              "condition": [
                "=",
                [
                  "field",
                  {{ field "tms_Task.epic" }},
                  null
                ],
                [
                  "field",
                  {{ field "tms_Epic.id" }},
                  {
                    "join-alias": "Tms Epic - Epic"
                  }
                ]
              ],
              "alias": "Tms Epic - Epic"
            }
          ],
          "expressions": {
            "PointsImputed": [
              "case",
              [
                [
                  [
                    "is-null",
                    [
                      "field",
                      {{ field "tms_Task.points" }},
                      null
                    ]
                  ],
                  1
                ]
              ],
              {
                "default": [
                  "field",
                  {{ field "tms_Task.points" }},
                  null
                ]
              }
            ]
          },
          "aggregation": [
            [
              "aggregation-options",
              [
                "/",
                [
                  "sum-where",
                  [
                    "expression",
                    "PointsImputed"
                  ],
                  [
                    "=",
                    [
                      "field",
                      {{ field "tms_Task.statusCategory" }},
                      null
                    ],
                    "Done"
                  ]
                ],
                [
                  "sum",
                  [
                    "expression",
                    "PointsImputed"
                  ]
                ]
              ],
              {
                "name": "Completion",
                "display-name": "Completion"
              }
            ]
          ],
          "filter": [
            "and",
            [
              "!=",
              [
                "field",
                {{ field "tms_Task.typeDetail" }},
                null
              ],
              "Epic"
            ],
            [
              "=",
              [
                "field",
                {{ field "tms_Task.source" }},
                null
              ],
              "Jira"
            ]
          ]
        }
      },
      "visualization_settings": {
        "stackable.stack_type": "stacked",
        "graph.dimensions": [
          "name"
        ],
        "series_settings": {
          "Completion": {
            "color": "#98D9D9"
          }
        },
        "progress.goal": 1,
        "column_settings": {
          "[\"name\",\"Completion\"]": {
            "number_style": "percent"
          }
        },
        "graph.metrics": [
          "Completion"
        ]
      }
    },
