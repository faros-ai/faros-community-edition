mutation insert_compute_Application_one(
$uid: String!,
$name: String!,
$platform: String!
) {
  insert_compute_Application_one(object: {
    uid: $uid,
    name: $name,
    platform: $platform
  },
  on_conflict: {
    constraint: compute_Application_pkey, 
    update_columns: [refreshedAt]
  }) 
  {
    uid,
		id
  }
}

